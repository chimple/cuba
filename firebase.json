{
  "hosting": [
    {
      "public": "build",
      "site": "cuba-stage-dev",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**/*.js",
          "destination": "**/*.js"
        },
        {
          "source": "**/*.jsc",
          "destination": "**/*.jsc"
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "headers": [
        {
          "source": "**/*.jsc",
          "headers": [
            {
              "key": "Content-Type",
              "value": "application/javascript"
            }
          ]
        }
      ]
    },
