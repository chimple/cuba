  "scripts": {
    "start": "craco start",
    "build": "GENERATE_SOURCEMAP=false craco build && npm run copy-build && npx @capawesome/cli manifests:generate --path build && cap sync && npm run build-storybook",
    "create-check-sum": "node scripts/check_sum_generator.js",
    "copy-build": "shx cp chimple-lib/*.js build/ && shx cp -r chimple-lib/src build/src && shx cp -r chimple-lib/assets/* build/assets/",
    "serve-build": "http-server build",
    "test": "craco test --transformIgnorePatterns 'node_modules/(?!(@ionic/react|@ionic/react-router|@ionic/core|@stencil/core|ionicons)/)'",
    "eject": "craco eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --output-dir build/storybook"
  },
